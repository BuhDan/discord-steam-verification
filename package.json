{
  "name": "discord-steam-verification",
  "version": "1.0.0",
  "description": "Verify your Discord users on first join to your server and check if they have a specific app/game on their Steam library!",
  "main": "index.js",
  "scripts": {
    "dev": "devScript",
    "compile": "rimraf dist && yarn --silent && tsc && devScript --copyOnly",
    "compile-linux": "rimraf dist && yarn --silent && tsc && rsync --recursive src/views/* dist/views/",
    "start": "cd dist && node index"
  },
  "keywords": [
    "discord",
    "steam",
    "verification"
  ],
  "author": "eggsywashere",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.13",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "consola": "^2.12.2",
    "dotenv": "^8.2.0",
    "eris": "^0.13.1",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "express-session": "^1.17.1",
    "steam-login": "^0.1.7"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "rsync": "^0.6.1",
    "ts-devscript": "^1.0.4"
  }
}
